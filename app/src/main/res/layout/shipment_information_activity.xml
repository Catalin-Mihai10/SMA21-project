<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#752424">

    <TextView
        android:id="@+id/textView"
        android:layout_width="326dp"
        android:layout_height="47dp"
        android:background="@drawable/ic_rounded_shapes"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:text="Alege metoda de plata"
        android:textAlignment="center"
        android:textColor="#752424"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.468" />

    <Button
        android:id="@+id/cardButton"
        android:layout_width="154dp"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_rounded_shapes"
        android:drawableStart="@drawable/ic_card"
        android:paddingStart="20dp"
        android:text="CARD"
        android:textColor="#752424"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/rambursButton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.596"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/backButton"
        android:layout_width="326dp"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_rounded_shapes"
        android:drawableStart="@drawable/ic_back"
        android:paddingStart="20dp"
        android:paddingEnd="40dp"
        android:text="INAPOI"
        android:textColor="#752424"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.419"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.704"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/rambursButton"
        android:layout_width="154dp"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_rounded_shapes"
        android:drawableStart="@drawable/ic_ramburs"
        android:paddingStart="20dp"
        android:text="RAMBURS"
        android:textColor="#752424"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cardButton"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.596"
        tools:ignore="TextContrastCheck" />

    <EditText
        android:id="@+id/tAdress"
        android:layout_width="326dp"
        android:layout_height="48dp"
        android:background="@drawable/ic_rounded_shapes"
        android:ems="10"
        android:hint="Introdu adresa"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="#752424"
        android:textColorHint="#752424"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.25"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/tPhone"
        android:layout_width="326dp"
        android:layout_height="48dp"
        android:background="@drawable/ic_rounded_shapes"
        android:ems="10"
        android:hint="Introdu numarul de telefon"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="#752424"
        android:textColorHint="#752424"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.146"
        tools:ignore="MissingConstraints" />

    <Spinner
        android:id="@+id/localitateSpinner"
        android:layout_width="111dp"
        android:layout_height="48dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.851"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.355"
        tools:ignore="SpeakableTextPresentCheck" />

    <Spinner
        android:id="@+id/JudetSpinner"
        android:layout_width="111dp"
        android:layout_height="48dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.144"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.356"
        tools:ignore="SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>