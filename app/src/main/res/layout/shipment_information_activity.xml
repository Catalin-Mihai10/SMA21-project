<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F3F1F1">

    <TextView
        android:id="@+id/textView"
        android:layout_width="285dp"
        android:layout_height="40dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="320dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="380dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:text="Alege metoda de plata"
        android:textAlignment="center"
        android:textColor="#D32F2F"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/cardButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="186dp"
        android:layout_marginTop="398dp"
        android:layout_marginEnd="86dp"
        android:layout_marginBottom="294dp"
        android:background="#FFFFFF"
        android:text="CARD"
        android:textColor="#CD5E5E"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/backButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="136dp"
        android:layout_marginTop="474dp"
        android:layout_marginEnd="136dp"
        android:layout_marginBottom="218dp"
        android:background="#FFFFFF"
        android:text="BACK"
        android:textColor="#CD5E5E"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/rambursButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="86dp"
        android:layout_marginTop="398dp"
        android:layout_marginEnd="186dp"
        android:layout_marginBottom="294dp"
        android:background="#FFFFFF"
        android:text="RAMBURS"
        android:textColor="#CD5E5E"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextContrastCheck" />

    <EditText
        android:id="@+id/tAdress"
        android:layout_width="289dp"
        android:layout_height="49dp"
        android:background="@drawable/ic_rounded_shapes"
        android:ems="10"
        android:hint="Enter adress"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="#CD5E5E"
        android:textColorHint="#CD5E5E"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="163dp" />

    <EditText
        android:id="@+id/tPhone"
        android:layout_width="289dp"
        android:layout_height="49dp"
        android:background="@drawable/ic_rounded_shapes"
        android:ems="10"
        android:hint="Enter phone"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="#CD5E5E"
        android:textColorHint="#CD5E5E"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="68dp" />

    <Spinner
        android:id="@+id/localitateSpinner"
        android:layout_width="111dp"
        android:layout_height="48dp"
        android:layout_marginStart="214dp"
        android:layout_marginTop="248dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="447dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <Spinner
        android:id="@+id/JudetSpinner"
        android:layout_width="111dp"
        android:layout_height="48dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="248dp"
        android:layout_marginEnd="213dp"
        android:layout_marginBottom="447dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>