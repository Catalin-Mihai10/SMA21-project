<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EFE9E9"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/meniuConstraint"
        android:layout_width="80dp"
        android:layout_height="732dp"
        android:layout_marginEnd="331dp"
        android:background="#CD5E5E"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:context=".MainActivity">

        <TextView
            android:id="@+id/tUlei"
            android:layout_width="78dp"
            android:layout_height="0dp"
            android:layout_marginEnd="2dp"
            android:gravity="center"
            android:text="Uleiuri"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tAnimale" />

        <TextView
            android:id="@+id/tAnimale"
            android:layout_width="78dp"
            android:layout_height="0dp"
            android:layout_marginEnd="2dp"
            android:gravity="center"
            android:text="Animale"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/tUlei"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tFructe" />

        <TextView
            android:id="@+id/tFructe"
            android:layout_width="78dp"
            android:layout_height="0dp"
            android:layout_marginEnd="2dp"
            android:gravity="center"
            android:text="Fructe"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/tAnimale"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tLegume" />

        <TextView
            android:id="@+id/tLegume"
            android:layout_width="78dp"
            android:layout_height="0dp"
            android:layout_marginEnd="2dp"
            android:gravity="center"
            android:text="Legume"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/tFructe"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tLactate" />

        <TextView
            android:id="@+id/tLactate"
            android:layout_width="78dp"
            android:layout_height="0dp"
            android:layout_marginEnd="2dp"
            android:gravity="center"
            android:text="lactate"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/tLegume"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tFainoase" />

        <TextView
            android:id="@+id/tFainoase"
            android:layout_width="78dp"
            android:layout_height="0dp"
            android:layout_marginEnd="2dp"
            android:gravity="center"
            android:text="Fainoase"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintBottom_toTopOf="@+id/tLactate"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>